{
    "name": "jmluang/logtailer-client-sdk",
    "description": "Logtailer Client SDK for PHP - Log aggregation service client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "JM Luang",
            "email": "jmluang@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Jmluang\\Logtailer\\": "src/Jmluang/Logtailer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmluang\\Logtailer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}